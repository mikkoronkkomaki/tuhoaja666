		"react": "^17.0.1",
		"react-datepicker": "4.1.1",
		"react-dom": "^17.0.1",
		"react-flip-move": "^3.0.4",
		"react-highlight.js": "^1.0.7",
		"shadow-cljs": "^2.9.10",
		"xmlhttprequest": "^1.8.0"
	},
	"devDependencies": {
		"concurrently": "^5.2.0",
		"html-webpack-plugin": "^3.2.0",
		"karma": "^6.0.0",
		"karma-chrome-launcher": "^3.1.0",
		"karma-cljs-test": "^0.1.0",
		"webpack": "^4.43.0",
